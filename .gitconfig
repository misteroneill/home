[user]
    name = Pat O'Neill
    email = pgoneill@gmail.com
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = true
    pager = true
[alias]
    br = branch
    st = status
    co = checkout
    ci = commit
    ft = fetch
    rb = rebase
    cl = clone
    a  = add
    ps = push
    pl = pull
[core]
    excludesfile = ~/.gitignore_global
[branch]
    autosetuprebase = always
    autosetupmerge = always
