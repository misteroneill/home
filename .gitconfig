[user]
	name = Pat O'Neill
	email = pgoneill@gmail.com
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = true
	pager = true
[core]
	excludesfile = ~/.gitignore
[branch]
	autosetuprebase = always
	autosetupmerge = false
[push]
	default = tracking
[alias]
	co = checkout
	st = status
	rb = rebase
	br = branch
	cm = commit
	ft = fetch
	ci = commit
